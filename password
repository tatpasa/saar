import random
password_sym="+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
password=""

n=int(input("Сколько символов должно быть в пароле:"))
while n == 0:
    print("нельзя 0 символов")
else:
    for x in range(n):
        password += random.choice(password_sym)
    print(password)
